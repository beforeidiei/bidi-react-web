{
  "name": "bidi",
  "version": "1.0.0",
  "description": "BIDI - A bucket list of things to do before you die.",
  "homepage": "https://github.com/beforeidiei/bidi-react-web",
  "repository": {
    "type": "git",
    "url": "https://github.com/beforeidiei/bidi-react-web.git"
  },
  "author": {
    "name": "Eric David Smith",
    "email": "erictherobot@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=6.4"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks --progress",
    "deploy": "cross-env NODE_ENV=production webpack --display-chunks --progress | test | firebase deploy",
    "clean": "del-cli target",
    "lint": "eslint -c .eslintrc.yml *.js src server",
    "prebuild": "run-s clean lint test",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start": "npm run server:dev",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start"
  },
  "dependencies": {
    "babel-polyfill": "~6.22.0",
    "classnames": "~2.2.5",
    "graceful-fs": "^4.1.8",
    "immutable": "~3.8.1",
    "react": "~15.4.2",
    "react-dom": "~15.4.2",
    "react-redux": "~5.0.2",
    "react-router": "~3.0.2",
    "react-router-redux": "~4.0.6",
    "react-share": "^1.10.0",
    "redux": "~3.6.0",
    "redux-thunk": "~2.2.0",
    "reselect": "~2.5.4"
  },
  "devDependencies": {
    "autoprefixer": "~6.7.0",
    "babel-core": "~6.22.1",
    "babel-eslint": "~7.1.1",
    "babel-loader": "~6.2.5",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-react": "~6.22.0",
    "babel-preset-react-optimize": "~1.0.1",
    "babel-preset-stage-0": "~6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "~3.1.4",
    "css-loader": "~0.26.1",
    "del-cli": "~0.2.0",
    "eslint": "~3.14.1",
    "eslint-plugin-react": "~6.9.0",
    "express": "~4.14.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "firebase": "~3.6.7",
    "firebase-tools": "3.2.1",
    "html-webpack-plugin": "~2.26.0",
    "jasmine-core": "~2.5.2",
    "karma": "~1.4.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~2.0.2",
    "minx": "https://github.com/r-park/minx.git",
    "morgan": "~1.7.0",
    "node-sass": "~4.3.0",
    "nodemon": "~1.11.0",
    "npm-run-all": "~4.0.1",
    "postcss-loader": "~1.2.2",
    "react-addons-test-utils": "~15.4.2",
    "react-hot-loader": "3.0.0-beta.5",
    "sass-loader": "~4.1.1",
    "sinon": "~1.17.6",
    "style-loader": "~0.13.1",
    "webpack": "~1.14.0",
    "webpack-dev-server": "~1.16.1",
    "webpack-md5-hash": "~0.0.5",
    "winston": "~2.3.1"
  }
